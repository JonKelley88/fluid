eft } = this.state;\n\t\treturn (\n\t\t\t<div>\n\t\t\t\t<label>\n\t\t\t\t\t{ property }\n\t\t\t\t</label>\n\t\t\t\t<div className={ `value ${property}` }>\n\t\t\t\t\t{ value }\n\t\t\t\t</div>\n\t\t\t\t<div\n\t\t\t\t\tclassName={ `slider ${property}` }\n\t\t\t\t\tonMouseDown={ this.onSliderDown }\n\t\t\t\t>\n\t\t\t\t\t<div className=\"bar\" />\n\t\t\t\t\t<div\n\t\t\t\t\t\tclassName={ `control ${dragging ? 'highlighted' : ''}` }\n\t\t\t\t\t\tonMouseDown={ this.onControlDown }\n\t\t\t\t\t\tstyle={ { left } }\n\t\t\t\t\t/>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t);\n\t}\n};"},"lineCount":null}},"hash":"c51ea13995a39a883fc42d4c8f00ce50","cacheData":{"env":{}}}