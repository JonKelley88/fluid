{
  "presets": [
  	"@babel/preset-react",
  	["@babel/preset-env", {
		"targets": "last 2 versions, not dead, ie 11"
  	}]
  ],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-syntax-object-rest-spread",
		["@babel/plugin-transform-runtime", {
			"useESModules": true
		}]
	]
}